<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
                                   "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">

<tiles-definitions>

	<definition name="cliente.main" extends="nib.main">
		<putList name="css">
			<item value="clienteCss" link="app/libs/css/cliente/cliente.css"
				tooltip="screen, print" />
		</putList>
		<putList name="js">
			<item value="clienteJs" link="app/libs/js/cliente/cliente.js" />
		</putList>
	</definition>

	<definition name="cliente.tiles" extends="cliente.main">
		<put name="subtitulo" value="Cliente" />
		<put name="body" value="/WEB-INF/jsp/cliente.jsp" />
	</definition>

	<definition name="manutencao.tiles" extends="cliente.main">
		<put name="subtitulo" value="Manutencao de Cliente" />
		<put name="body" value="/WEB-INF/jsp/manutencao.jsp" />
	</definition>

</tiles-definitions>